{
	"manifest_version": 2,
	"name": "Selfy Downloader",
	"version": "1.0",
	"icons":{
		"48":"icon/icon48.png",
		"16":"icon/icon16.png"
	},
	"browser_action":{
		"default_icon": "icon/icon16.png",	
		"default_popup": "popup.html"
	},
	"permissions": [
		"nativeMessaging",
		"notifications",
		"tabs",
		"storage"
	],
	"options_page": "login.html",
	"background":{ "scripts": ["background.js"]}
		
}